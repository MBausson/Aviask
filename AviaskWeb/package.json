{
    "name": "aviaskweb",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "prett": "npx prettier ./src --experimental-ternaries --write",
        "build-css": "npx tailwind -i ./src/assets/main.css -o ./public/out.css",
        "watch-css": "npx tailwind -i ./src/assets/main.css -o ./public/out.css --watch",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "lint": "npx eslint --ext js,vue,ts src",
        "lintfix": "npx eslint --ext js,vue,ts src --fix",
        "test": "vitest --dom"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.19",
        "@heroicons/vue": "^2.1.1",
        "@rushstack/eslint-patch": "^1.7.2",
        "@vue/eslint-config-typescript": "^13.0.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "eslint-config-prettier": "^9.1.0",
        "happy-dom": "^13.8.4",
        "isomorphic-dompurify": "^2.4.0",
        "js-cookie": "^3.0.5",
        "jwt-decode": "^4.0.0",
        "lucide-vue-next": "^0.379.0",
        "marked": "^12.0.1",
        "moment": "^2.30.1",
        "pinia": "^2.1.7",
        "radix-vue": "^1.8.1",
        "tailwind-merge": "^2.3.0",
        "tailwindcss-animate": "^1.0.7",
        "vite": "^5.1.6",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0",
        "vue3-apexcharts": "^1.5.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@tsconfig/node18": "^18.2.2",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^20.11.27",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.18",
        "axios": "^1.6.7",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "npm-run-all2": "^6.1.2",
        "postcss": "^8.4.35",
        "prettier": "3.2.5",
        "serve": "^14.2.1",
        "tailwindcss": "^3.4.1",
        "typescript": "~5.4.2",
        "vitest": "^1.3.1",
        "vue-tsc": "^2.0.6"
    }
}
